{
  "name": "@wesib/generic",
  "version": "1.0.0-beta.16",
  "description": "Wesib: Generic Components",
  "keywords": [
    "web-components",
    "library",
    "typescript"
  ],
  "homepage": "https://github.com/wesib/generic",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:wesib/generic.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin<ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/wesib/wesib/issues"
  },
  "main": "dist/wesib.generic.js",
  "module": "dist/wesib.generic.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/wesib.generic.mjs",
      "require": "./dist/wesib.generic.js"
    },
    "./input": {
      "import": "./dist/wesib.input.mjs",
      "require": "./dist/wesib.input.js"
    },
    "./styp": {
      "import": "./dist/wesib.styp.mjs",
      "require": "./dist/wesib.styp.js"
    }
  },
  "peerDependencies": {
    "@proc7ts/call-thru": "^4.1.0",
    "@proc7ts/context-values": "^4.3.0",
    "@proc7ts/fun-events": "^9.3.0",
    "@proc7ts/input-aspects": "^3.3.1",
    "@proc7ts/namespace-aliaser": "^2.1.0",
    "@proc7ts/render-scheduler": "^1.3.0",
    "@proc7ts/style-producer": "^5.3.0",
    "@wesib/wesib": "^1.0.0-beta.16"
  },
  "dependencies": {
    "@hatsy/http-header-value": "^3.4.0",
    "@proc7ts/a-iterable": "^2.8.0",
    "@proc7ts/primitives": "^1.2.0"
  },
  "peerDependenciesMeta": {
    "@proc7ts/input-aspects": {
      "optional": true
    },
    "@proc7ts/style-producer": {
      "optional": true
    }
  },
  "devDependencies": {
    "@proc7ts/call-thru": "^4.1.0",
    "@proc7ts/context-values": "^4.3.0",
    "@proc7ts/eslint-config": "^2.0.4",
    "@proc7ts/fun-events": "^9.3.0",
    "@proc7ts/input-aspects": "^3.3.1",
    "@proc7ts/namespace-aliaser": "^2.1.0",
    "@proc7ts/render-scheduler": "^1.3.0",
    "@proc7ts/rollup-helpers": "^1.0.0",
    "@proc7ts/style-producer": "^5.3.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/jest": "^26.0.13",
    "@types/jsdom": "^16.2.4",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@wesib/wesib": "^1.0.0-beta.16",
    "codecov": "^3.7.2",
    "cross-fetch": "^3.0.5",
    "cssesc": "^3.0.0",
    "eslint": "^7.8.1",
    "eslint-plugin-jest": "^24.0.0",
    "gh-pages": "^3.1.0",
    "hyphenate-style-name": "^1.0.4",
    "jest": "^26.4.2",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-junit": "^11.1.0",
    "rollup": "^2.26.10",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "run-z": "^1.1.1",
    "shx": "^0.3.2",
    "ts-jest": "^26.3.0",
    "tslib": "^2.0.1",
    "typedoc": "^0.19.0",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then rollup --config ./rollup.config.js",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf d.ts dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z +z --then eslint . --ext .js,.ts",
    "test": "run-z +z --then jest",
    "z": "run-z +build,+doc,+lint,+test"
  }
}
